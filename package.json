{
  "repository": "https://github.com/jpb06/effect-errors.git",
  "main": "dist/index.js",
  "name": "effect-errors",
  "version": "1.0.8",
  "author": "jpb06 <jp.bois.06@outlook.fr>",
  "description": "A POC for errors reporting in Effect",
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "update-deps": "bunx npm-check-updates --root --format group -i",
    "copy-package": "copyfiles package.json ./dist/",
    "copy-readme": "copyfiles README.md ./dist/",
    "build": "del-cli ./dist && tsc --project tsconfig-prod.json",
    "postbuild": "bun run copy-package && bun run copy-readme",
    "lint": "eslint ./src",
    "type-check": "tsc --noEmit",
    "sync-icons": "bun generateReadmeIcons -h 50",
    "run-examples": "for i in ./src/examples/*.ts; do bun run $i; done"
  },
  "dependencies": {
    "@effect/schema": "^0.63.2",
    "chalk": "^5.3.0",
    "dotenv-flow": "^4.1.0",
    "effect": "^2.4.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.6.3",
    "@types/eslint": "^8.56.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^3.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "fs-extra": "^11.2.0",
    "prettier": "^3.2.5",
    "readme-package-icons": "^1.1.14",
    "typescript": "^5.0.0"
  }
}
