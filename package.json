{
  "repository": "https://github.com/jpb06/effect-errors.git",
  "main": "index.js",
  "name": "effect-errors",
  "version": "1.3.5",
  "author": "jpb06 <jp.bois.06@outlook.fr>",
  "description": "A POC for errors reporting in Effect",
  "keywords": [],
  "license": "MIT",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "update-deps": "bunx npm-check-updates --root --format group -i",
    "copy-package": "copyfiles package.json ./dist/",
    "copy-readme": "copyfiles README.md ./dist/",
    "build": "del-cli ./dist && tsc --project tsconfig-prod.json",
    "postbuild": "bun run copy-package && bun run copy-readme",
    "lint": "eslint ./src",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test-ci": "vitest --coverage --run",
    "test-dev": "vitest --coverage",
    "test-coverage": "vitest run --coverage",
    "sync-icons": "bun generateReadmeIcons -h 50",
    "run-example": "bun run ./src/examples/util/run",
    "run-examples": "bun run ./src/examples/util/run-all"
  },
  "dependencies": {
    "@effect/schema": "^0.66.10",
    "chalk": "<5",
    "effect": "^3.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@stylistic/eslint-plugin": "^1.7.2",
    "@stylistic/eslint-plugin-ts": "^1.7.2",
    "@types/eslint": "^8.56.10",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "@vitest/coverage-v8": "^1.5.2",
    "copyfiles": "^2.4.1",
    "del-cli": "^5.1.0",
    "eslint": "^9.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^4.0.1",
    "eslint-plugin-n": "^17.3.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vitest": "^0.5.4",
    "fs-extra": "^11.2.0",
    "globals": "^15.1.0",
    "prettier": "^3.2.5",
    "readme-package-icons": "^1.1.14",
    "typescript": "*",
    "typescript-eslint": "^7.8.0",
    "vitest": "^1.5.2",
    "vitest-mock-extended": "^1.3.1"
  }
}
